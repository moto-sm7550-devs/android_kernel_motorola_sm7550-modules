#
# Focaltech Touchscreen driver configuration
#

config TOUCHSCREEN_FTS_V3_4
    bool "Focaltech Touchscreen v3"
    default n
    help
      Say Y here if you have Focaltech touch panel.
      If unsure, say N.

config TOUCHSCREEN_FTS_DIRECTORY_V3_4
    string "Focaltech ts v3.4 directory name"
    default "focaltech_touch_v3_4"
    depends on TOUCHSCREEN_FTS_V3_4
